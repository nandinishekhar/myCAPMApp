{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run BooksApp-1",
      "request": "launch",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/BooksApp",
      "runtimeExecutable": "npx",
      "args": [
        "cds",
        "run",
        "--in-memory?"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/BooksApp/.env1",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/BooksApp\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    },
    {
      "name": "Run EmployeeApp-1",
      "request": "launch",
      "type": "pwa-node",
      "cwd": "${workspaceFolder}/EmployeeApp",
      "runtimeExecutable": "npx",
      "args": [
        "cds",
        "run",
        "--in-memory?"
      ],
      "console": "internalConsole",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "envFile": "${workspaceFolder}/EmployeeApp/.env1",
      "env": {
        "NODE_ENV": "development",
        "run.config": "{\"handlerId\":\"cap_run_config_handler_id\",\"runnableId\":\"{\\\"projectPath\\\":\\\"/home/user/projects/EmployeeApp\\\",\\\"profileName\\\":\\\"development\\\"}\"}"
      }
    }
  ]
}